{
  "components": {
    "parameters": {
      "Account": {
        "description": "Account number",
        "in": "query",
        "name": "account",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "Application": {
        "description": "If set, content is associated with a specific CRC application",
        "explode": true,
        "in": "query",
        "name": "application",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "Bundle": {
        "description": "If set, content is associated with a specific CRC bundle",
        "explode": true,
        "in": "query",
        "name": "bundle",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "Content": {
        "description": "If set, content is associated with a specific CRC content",
        "explode": true,
        "in": "query",
        "name": "content",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "DisplayName": {
        "description": "Search quickstarts by display name",
        "explode": true,
        "in": "query",
        "name": "display-name",
        "required": false,
        "schema": {
          "type": "string"
        },
        "style": "form"
      },
      "Id": {
        "description": "identifier",
        "in": "path",
        "name": "id",
        "required": true,
        "schema": {
          "type": "integer"
        }
      },
      "Kind": {
        "description": "If set, content is associated with a specific kind",
        "explode": true,
        "in": "query",
        "name": "kind",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "Limit": {
        "description": "Pagination limit",
        "explode": true,
        "in": "query",
        "name": "limit",
        "required": false,
        "schema": {
          "type": "integer"
        },
        "style": "form"
      },
      "MaxDistance": {
        "description": "Maximum Levenshtein distance for fuzzy matching (default 3)",
        "explode": true,
        "in": "query",
        "name": "max_distance",
        "required": false,
        "schema": {
          "default": 3,
          "minimum": 0,
          "type": "integer"
        },
        "style": "form"
      },
      "Name": {
        "description": "Search content by name",
        "explode": true,
        "in": "query",
        "name": "name",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "Offset": {
        "description": "Pagination offset",
        "explode": true,
        "in": "query",
        "name": "offset",
        "required": false,
        "schema": {
          "type": "integer"
        },
        "style": "form"
      },
      "ProductFamilies": {
        "description": "If set, content is associated with a specific CRC product families",
        "explode": true,
        "in": "query",
        "name": "product-families",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "QuickstartName": {
        "description": "Search quickstarts by name",
        "explode": true,
        "in": "query",
        "name": "name",
        "required": false,
        "schema": {
          "type": "string"
        },
        "style": "form"
      },
      "SearchTerm": {
        "description": "Search term for fuzzy search",
        "explode": true,
        "in": "query",
        "name": "q",
        "required": true,
        "schema": {
          "type": "string"
        },
        "style": "form"
      },
      "Topic": {
        "description": "If set, content is associated with a specific topic",
        "explode": true,
        "in": "query",
        "name": "topic",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      },
      "TopicName": {
        "description": "identifier",
        "in": "path",
        "name": "name",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "UseCase": {
        "description": "If set, content is associated with a specific CRC use case",
        "explode": true,
        "in": "query",
        "name": "use-case",
        "required": false,
        "schema": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "style": "form"
      }
    },
    "schemas": {
      "BadRequest": {
        "properties": {
          "msg": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FavoriteQuickstart": {
        "properties": {
          "accountId": {
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "deletedAt": {},
          "favorite": {
            "type": "boolean"
          },
          "id": {
            "minimum": 0,
            "type": "integer"
          },
          "quickstartName": {
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "type": "object"
      },
      "FuzzySearchResult": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Quickstart"
          },
          {
            "properties": {
              "distance": {
                "description": "Levenshtein distance from the search term",
                "minimum": 0,
                "type": "integer"
              }
            },
            "required": [
              "distance"
            ],
            "type": "object"
          }
        ]
      },
      "HelpTopic": {
        "properties": {
          "content": {
            "format": "byte",
            "type": "object"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "deletedAt": {
            "format": "date-time",
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "type": "string"
          },
          "groupName": {
            "type": "string"
          },
          "id": {
            "minimum": 0,
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "tags": {
            "items": {
              "$ref": "#/components/schemas/Tag"
            },
            "type": "array"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "type": "object"
      },
      "NotFound": {
        "properties": {
          "msg": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "Quickstart": {
        "properties": {
          "content": {
            "format": "byte",
            "type": "object"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "deletedAt": {
            "format": "date-time",
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "type": "string"
          },
          "favoriteQuickstart": {
            "items": {
              "$ref": "#/components/schemas/FavoriteQuickstart"
            },
            "type": "array"
          },
          "id": {
            "minimum": 0,
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "tags": {
            "items": {
              "$ref": "#/components/schemas/Tag"
            },
            "type": "array"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "type": "object"
      },
      "QuickstartProgress": {
        "properties": {
          "accountId": {
            "type": "integer"
          },
          "progress": {
            "additionalProperties": true,
            "type": "object"
          },
          "quickstartName": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "QuickstartProgressRequest": {
        "properties": {
          "accountId": {
            "type": "integer"
          },
          "progress": {
            "additionalProperties": true,
            "type": "object"
          },
          "quickstartName": {
            "type": "string"
          }
        },
        "required": [
          "accountId",
          "quickstartName"
        ],
        "type": "object"
      },
      "Tag": {
        "properties": {
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "deletedAt": {
            "format": "date-time",
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "string"
              }
            ],
            "type": "string"
          },
          "id": {
            "minimum": 0,
            "type": "integer"
          },
          "type": {
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "info": {
    "license": {
      "name": "MIT"
    },
    "title": "quickstarts",
    "version": "1.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/favorites": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/Account"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "items": {
                        "$ref": "#/components/schemas/FavoriteQuickstart"
                      },
                      "type": "array"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON array of all favorites"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          }
        },
        "summary": "Returns list of all favorites"
      },
      "post": {
        "parameters": [
          {
            "$ref": "#/components/parameters/Account"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FavoriteQuickstart"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FavoriteQuickstart"
                }
              }
            },
            "description": "A JSON object with a single favorite content"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          }
        },
        "summary": "Add a favorite"
      }
    },
    "/helptopics": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/Bundle"
          },
          {
            "$ref": "#/components/parameters/Application"
          },
          {
            "$ref": "#/components/parameters/Name"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "items": {
                        "$ref": "#/components/schemas/HelpTopic"
                      },
                      "type": "array"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON array of all help topics"
          }
        },
        "summary": "Returns list of all help topics"
      }
    },
    "/helptopics/{name}": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/TopicName"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "$ref": "#/components/schemas/HelpTopic"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON of a help topic set"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            },
            "description": "Not found"
          }
        },
        "summary": "Return a help topics set by topic name"
      }
    },
    "/progress": {
      "get": {
        "parameters": [
          {
            "description": "Filter by account",
            "in": "query",
            "name": "account",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "Filter by quickstart name",
            "in": "query",
            "name": "quickstart",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "items": {
                        "$ref": "#/components/schemas/QuickstartProgress"
                      },
                      "type": "array"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON array of progress records"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          }
        },
        "summary": "Returns list of progress records"
      },
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/QuickstartProgressRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/QuickstartProgress"
                }
              }
            },
            "description": "Progress record created/updated"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          }
        },
        "summary": "Create or update progress record"
      }
    },
    "/progress/{id}": {
      "delete": {
        "parameters": [
          {
            "description": "Progress record ID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Progress record deleted"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            },
            "description": "Progress record not found"
          }
        },
        "summary": "Delete progress record by ID"
      }
    },
    "/quickstarts": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/ProductFamilies"
          },
          {
            "$ref": "#/components/parameters/Content"
          },
          {
            "$ref": "#/components/parameters/UseCase"
          },
          {
            "$ref": "#/components/parameters/Bundle"
          },
          {
            "$ref": "#/components/parameters/Application"
          },
          {
            "$ref": "#/components/parameters/Kind"
          },
          {
            "$ref": "#/components/parameters/Topic"
          },
          {
            "$ref": "#/components/parameters/QuickstartName"
          },
          {
            "$ref": "#/components/parameters/DisplayName"
          },
          {
            "$ref": "#/components/parameters/Limit"
          },
          {
            "$ref": "#/components/parameters/Offset"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "items": {
                        "$ref": "#/components/schemas/Quickstart"
                      },
                      "type": "array"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON array of all quickstarts"
          }
        },
        "summary": "Returns list of all quickstarts"
      }
    },
    "/quickstarts/filters": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "additionalProperties": true,
                      "type": "object"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON object with filter data"
          }
        },
        "summary": "Returns filters for quickstarts"
      }
    },
    "/quickstarts/fuzzy-search": {
      "get": {
        "description": "Performs fuzzy search on quickstart display names using Levenshtein distance for typo tolerance",
        "parameters": [
          {
            "$ref": "#/components/parameters/SearchTerm"
          },
          {
            "$ref": "#/components/parameters/MaxDistance"
          },
          {
            "$ref": "#/components/parameters/Limit"
          },
          {
            "$ref": "#/components/parameters/Offset"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "items": {
                        "$ref": "#/components/schemas/FuzzySearchResult"
                      },
                      "type": "array"
                    },
                    "max_distance": {
                      "description": "The maximum Levenshtein distance used",
                      "type": "integer"
                    },
                    "search_term": {
                      "description": "The search term used",
                      "type": "string"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "A JSON object with fuzzy search results"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Internal server error"
          }
        },
        "summary": "Search quickstarts using fuzzy matching"
      }
    },
    "/quickstarts/{id}": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/Id"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Quickstart"
                }
              }
            },
            "description": "A JSON object with a single quickstart content"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            },
            "description": "Bad request"
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            },
            "description": "Not found"
          }
        },
        "summary": "Return a quickstarts by ID"
      }
    }
  }
